{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TeamPortfolioDatabase": {
    "ConnectionString": "mongodb+srv://<username>:<password>@cluster0.mongodb.net/TeamPortfolio?retryWrites=true&w=majority",
    "DatabaseName": "TeamPortfolio",
    "TeamMembersCollectionName": "TeamMembers"
  },
  "JwtSettings": {
    "Secret": "your_very_strong_secret_key_at_least_32_characters_long",
    "ExpiryDays": 7,
    "Issuer": "PortfolioAPI",
    "Audience": "PortfolioAPI_Client"
  }
}